  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flokicoin Explorer (Backend)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/backend",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "dev"],
      "console": "integratedTerminal"
    },
    {
      "name": "Flokicoin Explorer (Frontend)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/frontend",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "serve"],
      "console": "integratedTerminal"
    }
  ]
}
